{
  "name": "mgmt-api",
  "version":"1.0.55",
  "description": "API for srs-terraform mgmt",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=development USE_DOCKER=false node .",
    "docker": "env NODE_ENV=development USE_DOCKER=true node .",
    "build": "cd ui && env PUBLIC_URL=/mgmt npm run build",
    "upgrade": "cd ui && env BUILD_PATH=./upgrade PUBLIC_URL=/mgmt npm run build",
    "postinstall": "cd ui && npm install"
  },
  "author": "ossrs",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "ioredis": "^4.28.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-proxies": "^0.12.1",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "moment": "^2.29.1",
    "semver": "^7.3.5"
  }
}
